programa {
  funcao inicio() {

    inteiro valor_inicial, valor_final

    escreva("valor inicial: ")
    leia(valor_inicial)

    escreva("valor final: ")
    leia(valor_final)

    para (inteiro contador = valor_inicial; contador <= valor_final ; contador++ ) {
      escreva (contador, "\n" )
    }
    
  }
}
